FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 280;

boundaryField
{
    floor
    floor
    floor
        value           uniform 5
    }
    ceiling
    {
        type            fixedValue;
        value           uniform 280;
    }
    fixedWalls
    {
        type            zeroGradient;
    }
    frontAndBack
    {
	type		empty;
    }
}

